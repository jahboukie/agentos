{
  "name": "agentos",
  "version": "0.1.0",
  "description": "The world's first operating system designed for AI coding agents",
  "main": "dist/index.js",
  "bin": {
    "agentos": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "node dist/index.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "coding",
    "development",
    "memory",
    "execution",
    "learning",
    "productivity",
    "automation",
    "ai-agents",
    "claude-code",
    "machine-learning",
    "code-prediction",
    "sandbox-execution",
    "persistent-memory"
  ],
  "author": "AgentOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jahboukie/agentos.git"
  },
  "bugs": {
    "url": "https://github.com/jahboukie/agentos/issues"
  },
  "homepage": "https://agentos.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dockerode": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/dockerode": "^3.3.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0"
  }
}
